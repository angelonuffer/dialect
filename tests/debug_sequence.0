dialect = ../code/0

test_sequence = dialect.analyze({
  input: "abc"
  grammar: {
    type: "sequence"
    parts: [
      { type: "symbol" text: "a" }
      { type: "symbol" text: "b" }
      { type: "symbol" text: "c" }
    ]
    format: "text"
  }
})

tester = formato => (
  "success: " + test_sequence.success + "\n" +
  "value: " + test_sequence.value + "\n"
)

tester
