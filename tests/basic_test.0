dialect = ../code/0
unitest = https://cdn.jsdelivr.net/gh/angelonuffer/unitest@c329fba680e4f77f514bf56990522199e3648b2c/code/0

result1 = dialect.analyze({
  input: "hello"
  grammar: {
    type: "symbol"
    text: "hello"
  }
})

result2 = dialect.analyze({
  input: "world"
  grammar: {
    type: "symbol"
    text: "hello"
  }
})

result3 = dialect.generate({
  value: "test"
  grammar: {
    type: "symbol"
    text: "test"
  }
})

unitest.describe({
  name: "Basic Tests"
  tests: [
    unitest.describe({
      name: "Symbol match success"
      tests: [
        unitest.equals([ result1.success 1 ])
        unitest.equals([ result1.value "hello" ])
      ]
    })
    unitest.describe({
      name: "Symbol match failure"
      tests: [
        unitest.equals([ result2.success 0 ])
      ]
    })
    unitest.describe({
      name: "Symbol generate"
      tests: [
        unitest.equals([ result3.success 1 ])
        unitest.equals([ result3.output "test" ])
      ]
    })
  ]
})
